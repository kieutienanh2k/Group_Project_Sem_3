﻿@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityRole>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@Html.ActionLink("Create New Role", "CreateRole", "Role")
<h2>RoleList</h2>
<table>
    <thead>
        <tr>
            <td>
                Role Name
            </td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        @item.Name
    </td>
</tr>}
    </tbody>
</table>
